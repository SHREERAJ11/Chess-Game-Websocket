{
  "version": 2,
  "builds": [
    {
      "src": "server/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "client/client.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "client/client.html"
    },
    {
      "src": "/api",
      "dest": "server/server.py"
    }
  ]
}
